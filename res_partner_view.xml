<?xml version="1.0"?>
<openerp>
    <data>
        <record id="view_partners_form_fnx_sr" model="ir.ui.view">
                <field name="name">view.res.partner.form.fnx.sr</field>
                <field name="model">res.partner</field>
                <field name="inherit_id" ref="base.view_partner_form"/>
                <field eval="18" name="priority"/>
                <field name="arch" type="xml">
                    <field name="supplier" position="after">
                        <field name="is_carrier" attrs="{'invisible=':[('is_company','=','False')]}"/>
                        <field name="is_driver" attrs="{'invisible=':[('is_company','=','True')]}"/>
                    </field>
                    <page name="page_history" position="before">
                        <page string="warehouse">
                            <field name="warehouse_comment" nolabel="1"/>
                        </page>
                    </page>
                </field>
        </record>

    </data>

</openerp>
